#: ../data/mousetweaks.ui.h:1
msgid "Hover Click"
msgstr "کلیک شناور"

#: ../data/mousetweaks.ui.h:2
msgid "Single Click"
msgstr "یک کلیک"

#: ../data/mousetweaks.ui.h:3
msgid "Double Click"
msgstr "دوبار کلیک"

